{
  "name": "@statisticsnorway/ssb-component-library",
  "version": "2.5.2",
  "description": "Component library for SSB (Statistics Norway)",
  "main": "lib/bundle.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build:prod": "cross-env ENV=production rollup -c --bundleConfigAsCjs",
    "start": "cross-env ENV=development storybook dev -p 6006",
    "test": "cross-env ENV=test jest --coverage",
    "test:watch": "cross-env ENV=test jest --watch",
    "clean-test": "cross-env ENV=test jest --no-cache --updateSnapshot --coverage",
    "clear-tests": "jest --clear-cache -- -u",
    "lint:js": "eslint src/**/*.{jsx,tsx}",
    "lint:js:fix": "eslint --fix src/**/*.{jsx,tsx}",
    "lint:css": "stylelint src/components/**/*.scss",
    "lint:css:fix": "stylelint --fix src/components/**/*.scss",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "format:fix": "npx prettier -w .",
    "deploy": "npm publish",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/statisticsnorway/ssb-component-library.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/statisticsnorway/ssb-component-library/issues"
  },
  "sideEffects": false,
  "homepage": "https://statisticsnorway.github.io/design-system/",
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "~7.28.3",
    "@babel/core": "~7.28.4",
    "@babel/eslint-parser": "~7.28.4",
    "@babel/preset-env": "~7.28.3",
    "@babel/preset-react": "~7.27.1",
    "@babel/preset-typescript": "~7.27.1",
    "@rollup/plugin-babel": "~6.1.0",
    "@rollup/plugin-commonjs": "~28.0.6",
    "@rollup/plugin-node-resolve": "~16.0.3",
    "@rollup/plugin-replace": "~6.0.2",
    "@rollup/plugin-terser": "~0.4.4",
    "@rollup/plugin-typescript": "~12.1.4",
    "@storybook/addon-links": "^9.1.10",
    "@storybook/addon-webpack5-compiler-babel": "~3.0.6",
    "@storybook/react-webpack5": "^9.1.10",
    "@svgr/rollup": "~8.1.0",
    "@svgr/webpack": "~8.1.0",
    "@testing-library/dom": "~10.4.1",
    "@testing-library/jest-dom": "~6.9.1",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~14.6.1",
    "@types/jest": "~30.0.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "babel-jest": "~30.2.0",
    "babel-loader": "~10.0.0",
    "babel-plugin-module-resolver": "~5.0.2",
    "babel-plugin-prismjs": "~2.1.0",
    "cross-env": "~10.1.0",
    "css-loader": "~7.1.2",
    "eslint": "~8.57.1",
    "eslint-config-airbnb": "~19.0.4",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jsx-a11y": "~6.10.2",
    "eslint-plugin-prettier": "~5.5.4",
    "eslint-plugin-react": "~7.37.5",
    "eslint-webpack-plugin": "~5.0.2",
    "file-loader": "~6.2.0",
    "husky": "~9.1.7",
    "jest": "~30.2.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-fail-on-console": "~3.3.1",
    "path": "~0.12.7",
    "postcss": "~8.5.3",
    "prettier": "^3.6.2",
    "prop-types": "~15.8.1",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "rollup": "~4.52.4",
    "rollup-plugin-scss": "~4.0.1",
    "sass-loader": "~16.0.5",
    "storybook": "^9.1.10",
    "style-loader": "~4.0.0",
    "stylelint": "~16.25.0",
    "stylelint-config-standard-scss": "~15.0.1",
    "ts-jest": "~29.4.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.46.1",
    "webpack": "~5.102.1",
    "webpack-cli": "~6.0.1"
  },
  "dependencies": {
    "prismjs": "~1.30.0",
    "react-feather": "~2.0.10",
    "sass": "~1.86.1"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/bundle.css",
    "src/**/*.scss",
    "src/fonts/*"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "contributors": [
    {
      "name": "Kristoffer Lund"
    },
    {
      "name": "Carina G. Nordseth"
    }
  ]
}
